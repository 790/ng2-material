{
  "name": "ng2-material-docs",
  "version": "0.0.0",
  "description": "Documentation app for ng2-material",
  "repository": {
    "type": "git",
    "url": "https://github.com/justindujardin/ng2-material.git"
  },
  "scripts": {
    "postinstall": "typings install",
    "render": "npm run render:gulp",
    "render:gulp": "./node_modules/.bin/gulp prerender",
    "watch": "webpack --watch",
    "prebuild": "rimraf dist",
    "build": "webpack",
    "prestart": "npm run build",
    "server": "nodemon dist/server/bundle.js",
    "start": "npm run server",
    "predebug": "npm run build",
    "debug": "node --debug-brk dist/server/bundle.js",
    "style:w": "npm run style && node-sass -w src -o src",
    "style": "node-sass src/app.scss src/app.css"
  },
  "license": "MIT",
  "contributors": [],
  "dependencies": {
    "es6-promise": ">=3.0.2",
    "es6-shim": ">=0.35.0",
    "reflect-metadata": "0.1.2",
    "zone.js": ">=0.6.6",
    "angular2-universal": "~0.90.2",
    "body-parser": "^1.15.0",
    "express": "^4.13.3",
    "preboot": "^2.0.5",
    "rxjs": "5.0.0-beta.2",
    "angular2": "^2.0.0-beta.14"
  },
  "devDependencies": {
    "angular2-gulp-prerender": "^0.10.1",
    "connect-history-api-fallback": "^1.2.0",
    "gulp": "^3.9.1",
    "highlightjs": "8.7.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.8.1",
    "rimraf": "^2.5.0",
    "serve-static": "^1.10.2",
    "systemjs": "0.19.2",
    "ts-loader": "^0.8.1",
    "ts-node": "^0.7.1",
    "typescript": "^1.8.7",
    "typings": "^0.7.9",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  }
}
